<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE class SYSTEM "config.dtd">
<class>
	<directory-path>C:/users/</directory-path>
	<field setter="true"
		   getter="true"
		   scope="private"
		   javaType="Integer"
		   staticVar="true"
		   finalVar="true"
		   showDescription="true"/>

	<custom>
		<custom-fields>
			<custom-field setter="true"
					getter="true"
					scope="public"
					javaType="String"
				    staticVar="true"
				    finalVar="true"
					allClass="false"
					>
				<!--allClass 默认true 表示 所有类中都会生成这个自定义属性，false 表示只在有@customer注解的类中才会生成-->
				<field-name>LOCAL_PATH</field-name>
				<field-value>111</field-value>
				<field-description>本地路径</field-description>
			</custom-field>
			<custom-field setter="true"
					getter="true"
					scope="public"
					javaType="String"
				    staticVar="true"
				    finalVar="true"
					>
				<field-name>CLIENT_PATH</field-name>
				<field-value>111</field-value>
				<field-description>远程路径</field-description>
			</custom-field>
		</custom-fields>
		<custom-methods>
			<custom-method scope="public"
					javaType="void"
				    staticVar="true"
				    finalVar="true"
					allClass="true"
					>
<!--				#默认true 表示 所有类中都会生成这个自定义属性，false 表示只在有@customer注解的类中才会生成-->
				<method-name>getOrder</method-name>
				<method-annotations><!--方法上面的注解-->
					<method-annotation>@Override</method-annotation>
					<method-annotation>@JSONField(serialize = false)</method-annotation>
				</method-annotations>
				<method-return>LoanApplyResponse.class</method-return> <!--方法的返回值-->
				<method-params>
					<method-param javaType="int" finalVar="true">count</method-param>
					<method-param javaType="String" finalVar="true">name</method-param>
				</method-params>
			</custom-method>
			<custom-method scope="public"
						   javaType="void"
						   staticVar="true"
						   finalVar="true"
						   allClass="true"
			>
				<!--				#默认true 表示 所有类中都会生成这个自定义属性，false 表示只在有@customer注解的类中才会生成-->
				<method-name>loanOrder</method-name>
				<method-annotations> <!--方法上面的注解-->
					<method-annotation>@Override</method-annotation>
				</method-annotations>
				<method-return>LoanApplyResponse.class</method-return> <!--方法的方绘制-->
				<method-params>
					<method-param javaType="int" finalVar="true">count</method-param>
					<method-param javaType="String" finalVar="true">name</method-param>
				</method-params>
			</custom-method>
		</custom-methods>
	</custom>

</class>
